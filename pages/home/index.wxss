.dashboard {
  height: 100%;
  width: 100%;
  background: linear-gradient(180deg, #C3DFFF 0%, #F5F6F8 39%);
  padding: 32rpx;
  overflow: auto;
  box-sizing: border-box;
}
.banner, swiper{
  width: 100%;
}
.banner swiper-item {
  display: flex;
  justify-content: center;
  align-items: center;
}
.banner image{
  height: 100%;
}
.card-box {
  display: flex;
  margin-top: 20rpx;
}
.card-box .card-item {
  /* height: 188rpx; */
  flex: 1;
  padding: 20rpx;
  border-radius: 6px;
}
.card-box .card-item:nth-child(1) {
  background: url('data:image/svg+xml;base64,') no-repeat right bottom,linear-gradient(180deg, #C9E0FF 0%, #FFFFFF 100%);
  margin-right: 20rpx;
}

.card-box .card-item:nth-child(2) {
  background: url('data:image/svg+xml;base64,')  no-repeat right bottom,linear-gradient(180deg, #D9DAFF 0%, #FFFFFF 100%);
}

.card-box .card-item .title{
  font-size: 32rpx;
  font-weight: bold;
  color: #000000;
  line-height: 40rpx;
}
.card-box .card-item .desc{
  font-size: 24rpx;
  color: #5A5F65;
  line-height: 28rpx;
}


.card-box .card-item .btn{ 
  margin-top: 26rpx;
  border-radius: 42px 42px 42px 42px;
  opacity: 1;
  border: 1px solid #165DFF;
  font-size: 20rpx;
  display: inline-block;
  padding: 6rpx 24rpx;
  color: #165DFF;
}
.card-box .card-item:nth-child(2) .btn{
  border-color:#3C0CFF;
  color: #3C0CFF;
}
.tab-box {
  margin-top: 20rpx;
  background: #FFFFFF;
  border-radius: 12rpx;
}
.tab-box .tab-head{
  display: flex;
}

.tab-box .tab-head .tab-item{
  flex: 1;
  display: flex;
  height: 80rpx;
  justify-content: center;
  align-items: center;
}



.tab-box .tab-head .tab-item .tab-title{
  font-weight: 500;
  color: #858585;
  line-height: 40rpx;
  font-size: 32rpx;
}
.tab-box .tab-head .active-tab {
  background: linear-gradient(180deg, #D4E1FF 0%, #FFFFFF 100%);
  font-weight: bold;
  border-radius: 12rpx;
  position: relative;
}
.tab-box .tab-head .active-tab .tab-title {
  color: #000000;
}
.tab-box .tab-head .active-tab::after {
    content: "";
    display: block;
    position: absolute;
    width: 32px;
    height: 2px;
    bottom: 0;
    left: 50%;
    transform: translate(-50%,0);
    background: #165DFF;
    border-radius: 0px 0px 4px 4px;
}
.tab-box .tab-content {
  padding: 20rpx 24rpx 40rpx;
}
 
.tab-box .tab-content .tips{
  display: flex;
  align-items: center;
  font-size: 24rpx;
  font-weight: 500;
  color: #FF6D24;
  line-height: 28rpx;
}
.tab-box .tab-content .tips image{
  height: 32rpx;
  width: 32rpx;
  margin-right: 8rpx;
}
.tab-box .tab-content .input-model{
  margin-top: 16rpx;
  display: flex;
  align-items: center;
  padding: 20rpx;
  border: 1px solid #165DFF;
  border-radius: 8rpx;
  margin-right: 8rpx;
  font-size: 28rpx;
  font-weight: 500;
  color: #060606;
  line-height: 32rpx;
}
.tab-box .tab-content .input-model .short-line{ 
  width: 1px;
  height: 12px;
  background: #D9D9D9;
  margin: 0 20rpx;
}
.tab-box .tab-content .input-model .place-text{ 
  color: #BDBDBD;
}
.push-head {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin: 32rpx 0 20rpx;
}
.push-head .push-title {
  font-size: 32rpx;
  font-weight: bold;
  color: #000000;
}
.push-head .type-box {
  display: flex;
}
.push-head .type-box .push-item {
  padding: 10rpx 20rpx;
  border-radius: 8rpx;
  color: #86909C;
  font-size: 24rpx;
  background: #FFF;
  margin-left: 16rpx;
}
.push-head .type-box .active-push { 
  background: rgba(37,105,255,0.05);
  color: #165DFF;
}
.push-time {
  display: flex;
  margin-bottom: 20rpx;
}
.push-time .label{
  display: flex;
  color: #8A8A8A;
  font-size: 24rpx;
}
.push-time .time{ 
  margin-left: 4px;
  color: #FF1616;
  font-size: 24rpx;
}
.list .list-item {
  margin-bottom: 20rpx;
}